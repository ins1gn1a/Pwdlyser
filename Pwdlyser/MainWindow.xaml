<Window x:Class="Pwdlyser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="Pwdlyser"
        Name="PwdlyserMainWindow"
        Height="1114"
        Width="1700"
        MinHeight="800"
        MinWidth="1100"
        WindowState="Maximized"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="White"
        TextElement.FontWeight="Light"
        TextElement.FontSize="25"
        FontFamily="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto"
        WindowStartupLocation="CenterScreen">

    <Grid>
        <UniformGrid Columns="4">
            <FrameworkElement x:Name="c1" />

        </UniformGrid>
        <TabControl HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    TabStripPlacement="Top"
                    Margin="00"
                    BorderThickness="0"
                    Name="MainTabControl">

            <TabControl.Resources>
                <Style TargetType="TabItem">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="TabItem">
                                <Grid Name="Panel">
                                    <ContentPresenter x:Name="ContentSite"
                                                      VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"
                                                      ContentSource="Header"
                                                      Margin="10,2" />
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsSelected"
                                             Value="True">
                                        <Setter TargetName="Panel"
                                                Property="Background"
                                                Value="#E0E0E0" />

                                    </Trigger>
                                    <Trigger Property="IsSelected"
                                             Value="False">
                                        <Setter TargetName="Panel"
                                                Property="Background"
                                                Value="White" />
                                    </Trigger>
                                    <Trigger Property="IsMouseOver"
                                             Value="True">
                                        <Setter TargetName="Panel"
                                                Property="Background"
                                                Value="#03A9F4" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>

                </Style>
            </TabControl.Resources>
            <TabItem Header="Welcome"
                     Width="{Binding ElementName=c1, Path=Width}"
                     Margin="-2,-2,2,2"
                     Name="TabItemDashboard">
                <ScrollViewer VerticalScrollBarVisibility="Auto">

                    <Grid Background="White">
                        <StackPanel Margin="0,0,0,110"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Stretch">

                            <StackPanel VerticalAlignment="Top"
                                        Grid.Column="0">
                                <Viewbox Margin="150,0,150,10"
                                         MaxHeight="150">
                                    <Grid HorizontalAlignment="Center">
                                        <Image HorizontalAlignment="Left"
                                               Width="150"
                                               Height="150"
                                               Source="/image/pwdlyser-icon.png" />
                                        <Label Content="Pwdlyser"
                                               FontWeight="Bold"
                                               FontSize="115"
                                               Margin="220,50,0,0" />
                                        <Label Content=""
                                               VerticalAlignment="Bottom"
                                               FontWeight="Bold"
                                               FontSize="22"
                                               HorizontalContentAlignment="Center"
                                               Margin="0,200,0,0" />
                                    </Grid>
                                </Viewbox>
                            </StackPanel>
                            <StackPanel Grid.Column="1">
                                <RichTextBox FontSize="18"
                                             Margin="200,10,200,0"
                                             VerticalAlignment="Center"
                                             BorderThickness="0"
                                             IsReadOnly="True">
                                    <FlowDocument>
                                        <Paragraph FontFamily="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto">
                                            <Run Text='Get started with automated password analysis by pressing the "Get Started" button below and then importing a list of plaintext usernames and passwords and then pressing "Run Analysis". The default analysis options will be used, which can be changed by navigating to "Settings" and making the desired changes. These areas of analysis include the following:' />

                                        </Paragraph>
                                        <List>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Password lengths by usage frequency" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Most frequently used passwords, such as 'Password' or 'Welcome'" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Passwords that utilise common base words" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="User accounts that use a keyboard pattern" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Company name found within passwords" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Administrator or service account passwords that were obtained" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Passwords that contain a variation of the username" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Passwords that are reused among similarly named accounts" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Hashcat mask analysis for most commonly used masks" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Password history analysis on accounts that repeatedly re-use the same password" />
                                                </Paragraph>
                                            </ListItem>
                                            <ListItem>
                                                <Paragraph>
                                                    <Run Text="Individual character and character-set analysis of the entire password list" />
                                                </Paragraph>
                                            </ListItem>
                                        </List>
                                        <Paragraph>
                                            <LineBreak />
                                            <Run Text="Plaintext passwords can be obtained by performing offline password cracking of stored hashes, such as from an Active Directory or database output. Recommendations for password cracking tools include Hashcat and John-The-Ripper." />
                                        </Paragraph>
                                    </FlowDocument>

                                </RichTextBox>
                            </StackPanel>

                        </StackPanel>


                        <Button Content="Get Started"
                                Margin="150,0,150,22"
                                VerticalAlignment="Bottom"
                                x:Name="ButtonGetStarted"
                                MaxHeight="106"
                                MinHeight="80"
                                FontSize="18"
                                Foreground="White"
                                Click="ButtonGetStarted_Click" />
                    </Grid>
                </ScrollViewer>
            </TabItem>

            <TabItem Header="Analysis"
                     Width="{Binding ElementName=c1, Path=Width}"
                     HorizontalAlignment="Left"
                     Margin="-2,-2,0,2"
                     Name="TabAnalysis">
                <Grid Background="White"
                      TextElement.FontSize="14"
                      TextElement.FontWeight="Light"
                      VerticalAlignment="Stretch"
                      HorizontalAlignment="Stretch">
                    <DockPanel Grid.Row="0"
                               Margin="0,0,0,90"
                               DockPanel.Dock="Top">

                        <TabControl TabStripPlacement="Top"
                                    BorderThickness="0"
                                    Name="AnalysisTabControl"
                                    DockPanel.Dock="Top"
                                    HorizontalAlignment="Stretch"
                                    HorizontalContentAlignment="Stretch">

                            <!--Height="{Binding Path=ActualHeight,RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type Window}}}">-->
                            <TabItem Header="Overview"
                                     Width="{Binding Width, ElementName=c1}">

                                <Grid Margin="52,10,52,0"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Stretch">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="auto" />

                                    </Grid.ColumnDefinitions>

                                    <Label Content="Anaylsis Over Time"
                                           VerticalAlignment="Top"
                                           FontWeight="Bold"
                                           FontSize="25"
                                           Height="40" />
                                    <lvc:CartesianChart Grid.Column="0"
                                                        Grid.RowSpan="6"
                                                        Grid.Row="0"
                                                        Series="{Binding ChartTotalCrackedSeriesCollection}"
                                                        LegendLocation="None"
                                                        Name="ChartTotalCracked"
                                                        Visibility="Visible"
                                                        Margin="0,50,0,0"
                                                        MinWidth="500"
                                                        MinHeight="500"
                                                        MouseDown="ChartTotalCracked_MouseDown">
                                        <lvc:CartesianChart.ToolTip>
                                            <ToolTip>
                                                <TextBlock HorizontalAlignment="Center"
                                                           VerticalAlignment="Center"
                                                           Text="Click the graph to toggle project options." />
                                            </ToolTip>
                                        </lvc:CartesianChart.ToolTip>
                                        <lvc:CartesianChart.AxisY>
                                            <lvc:Axis Title="Percentage Cracked"
                                                      Name="ChartTotalYAxis"
                                                      LabelFormatter="{Binding ChartTotalCrackedFormatter}"
                                                      MaxValue="100"
                                                      MinValue="0"></lvc:Axis>
                                        </lvc:CartesianChart.AxisY>

                                    </lvc:CartesianChart>
                                    <Grid Grid.Column="1"
                                          Grid.ColumnSpan="2"
                                          Name="OverviewHiddenTab"
                                          Visibility="Collapsed">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="auto"
                                                              Name="GridAnalysisOverTime1" />

                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />

                                            <RowDefinition Height="auto" />
                                        </Grid.RowDefinitions>
                                        <Button Grid.Column="1"
                                                Grid.Row="4"
                                                FontSize="18"
                                                Content="Add Record"
                                                Height="59"
                                                x:Name="ButtonRunTotal"
                                                Foreground="White"
                                                MinWidth="150"
                                                Margin="10"
                                                Click="ButtonRunTotal_Click" />
                                        <Button Grid.Column="1"
                                                Grid.Row="5"
                                                FontSize="18"
                                                Content="Select Project"
                                                Height="59"
                                                Foreground="White"
                                                MinWidth="150"
                                                Margin="10"
                                                Name="TotalSelectFile"
                                                Click="TotalSelectFile_Click" />
                                        <Button Grid.Column="0"
                                                Grid.Row="5"
                                                FontSize="18"
                                                Content="New Project"
                                                Height="59"
                                                Foreground="White"
                                                MinWidth="150"
                                                Margin="10"
                                                Name="TotalAddProject"
                                                Click="TotalAddProject_Click" />

                                        <TextBlock Grid.Column="0"
                                                   Grid.Row="2"
                                                   TextWrapping="WrapWithOverflow"
                                                   FontSize="18"
                                                   Text="Total Percentage Cracked:"
                                                   VerticalAlignment="Center"
                                                   Margin="0,0,20,0" />

                                        <TextBox Grid.Column="1"
                                                 Grid.Row="2"
                                                 MinWidth="50"
                                                 HorizontalAlignment="Stretch"
                                                 Height="32"
                                                 Margin="20,10,20,0"
                                                 TextWrapping="Wrap"
                                                 Text="100"
                                                 VerticalAlignment="Center"
                                                 x:Name="TextBoxTotalManualAdd"
                                                 FontSize="18"
                                                 TextAlignment="Center"
                                                 PreviewTextInput="NumberValidationTextBox" />

                                        <TextBlock Grid.Column="0"
                                                   Grid.Row="3"
                                                   TextWrapping="WrapWithOverflow"
                                                   FontSize="18"
                                                   Text="Analysis Date:"
                                                   VerticalAlignment="Center"
                                                   Margin="0,0,20,0" />


                                        <TextBox Grid.Column="1"
                                                 Grid.Row="3"
                                                 MinWidth="50"
                                                 HorizontalAlignment="Stretch"
                                                 Margin="20,10,20,0"
                                                 VerticalAlignment="Center"
                                                 Name="TextBoxTotalManualAddDate"
                                                 FontSize="18"
                                                 TextAlignment="Center" />

                                        <DataGrid Name="DataGridTotalCracked"
                                                  Margin="0,60,0,20"
                                                  AutoGenerateColumns="False"
                                                  Grid.Column="0"
                                                  Grid.ColumnSpan="2"
                                                  HorizontalAlignment="Stretch"
                                                  CanUserSortColumns="True"
                                                  IsReadOnly="True"
                                                  ItemsSource="{Binding}"
                                                  FontFamily="Courier New"
                                                  CanUserAddRows="True"
                                                  CanUserDeleteRows="False"
                                                  CanUserReorderColumns="False">
                                            <DataGrid.Columns>
                                                <DataGridTextColumn Header="% Total Cracked/Analysed"
                                                                    Binding="{Binding total}" />
                                                <DataGridTextColumn Header="Date"
                                                                    Binding="{Binding date}" />
                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </Grid>
                                </Grid>


                            </TabItem>
                            <TabItem Header="Settings"
                                     Width="{Binding Width, ElementName=c1}">
                                <ScrollViewer VerticalScrollBarVisibility="Auto">
                                    <StackPanel ScrollViewer.VerticalScrollBarVisibility="Auto">
                                        <Label Content="Analysis Settings"
                                               VerticalAlignment="Top"
                                               FontWeight="Bold"
                                               FontSize="25"
                                               Height="40"
                                               Margin="52,10" />

                                        <StackPanel Grid.Column="0">
                                            <TextBlock TextWrapping="WrapWithOverflow"
                                                       FontSize="18"
                                                       Text="Select the options below to enable the various analysis checks that are performed by Pwdlyser."
                                                       Margin="52,10" />
                                        </StackPanel>
                                        <StackPanel Grid.Column="1">
                                            <WrapPanel HorizontalAlignment="Center"
                                                       VerticalAlignment="Top"
                                                       Margin="52,10,52,0">
                                                <CheckBox Content="Password/Suffix Frequency"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisFrequency"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisFrequency_Click" />
                                                <CheckBox Content="Password Length"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisLength"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisLength_Click" />
                                                <CheckBox Content="Character Analysis"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisCharacter"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisCharacter_Click" />
                                                <CheckBox Content="Dictionary Words"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisCommon"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisCommon_Click" />
                                                <CheckBox Content="Re-use / Sharing"
                                                          Width="270"
                                                          Height="41"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisReuse"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisReuse_Click" />
                                                <CheckBox Content="Keyboard Walking"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisKeyboard"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisKeyboard_Click" />
                                                <CheckBox Content="Date/Time Variation"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisDate"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisDate_Click" />
                                                <CheckBox Content="Company Name"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisCompany"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisCompany_Click" />
                                                <CheckBox Content="Administrator Accounts"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisAdministrator"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisAdministrator_Click" />
                                                <CheckBox Content="Usernames in Passwords"
                                                          IsChecked="True"
                                                          x:Name="CheckAnalysisUsername"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisUsername_Click" />

                                                <CheckBox Content="Password History"
                                                          x:Name="CheckAnalysisHistory"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          IsChecked="True"
                                                          Height="41"
                                                          VerticalAlignment="Top"
                                                          HorizontalAlignment="Left"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisHistory_Click" />
                                                <CheckBox Content="Weak Passwords"
                                                          Name="CheckAnalysisWeak"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisWeak_Click"
                                                          IsChecked="true" />
                                                <CheckBox Content="Passwords in Data Leaks"
                                                          Name="CheckAnalysisPwned"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisPwned_Checked"
                                                          IsChecked="true" />
                                                <CheckBox Content="Hashcat Mask Analysis"
                                                          x:Name="CheckAnalysisHashcat"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          IsChecked="True"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          Click="CheckAnalysisHashcat_Click" />
                                                <CheckBox Content="Include Historic Results *"
                                                          IsChecked="False"
                                                          x:Name="CheckIncludeHistory"
                                                          VerticalContentAlignment="Center"
                                                          FontSize="20"
                                                          Height="41"
                                                          Width="270"
                                                          Margin="10,5"
                                                          ToolTip="* Please note that included historic user account passwords in the analysis will list any Active Directory accounts that contain '_history. Analysis will be performed against all accounts and all accounts with historical records will be included in the output. It is not recommended to include this for final reports." />

                                            </WrapPanel>
                                        </StackPanel>
                                        <StackPanel Grid.Column="2"
                                                    VerticalAlignment="Top">
                                            <Grid VerticalAlignment="Top"
                                                  HorizontalAlignment="Center">
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <Button Content="Enable All"
                                                            Height="59"
                                                            x:Name="ButtonSettingsEnableAll"
                                                            Click="ButtonSettingsEnableAll_Click"
                                                            Foreground="White"
                                                            Width="254"
                                                            Margin="20"
                                                            ToolTip="Enable all analysis checks." />
                                                    <Button Content="Disable All"
                                                            Height="59"
                                                            x:Name="ButtonSettingsDisableAll"
                                                            Click="ButtonSettingsDisableAll_Click"
                                                            Foreground="White"
                                                            Width="254"
                                                            Margin="20"
                                                            ToolTip="Disable all analysis checks." />
                                                </WrapPanel>
                                            </Grid>
                                        </StackPanel>
                                        <StackPanel Grid.Column="3"
                                                    VerticalAlignment="Top">
                                            <Grid HorizontalAlignment="Center"
                                                  VerticalAlignment="Top">

                                            </Grid>
                                        </StackPanel>

                                        <StackPanel Grid.Column="0"
                                                    Margin="0,20">

                                            <Grid Margin="52,20">
                                                <TextBlock TextWrapping="WrapWithOverflow"
                                                           FontSize="18"
                                                           Text="The metrics below provide an overview of the time taken to perform each segment of the analysis and the number of entries for each of the results." />
                                            </Grid>
                                        </StackPanel>

                                        <StackPanel Grid.Column="1"
                                                    Margin="0,10">

                                            <WrapPanel Orientation="Horizontal"
                                                       HorizontalAlignment="Center">
                                                <StackPanel Grid.Column="1">


                                                    <Grid Width="700"
                                                          Margin="0,5">
                                                        <TextBlock TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="253"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="Frequency / Suffix Analysis"
                                                                   FontWeight="Bold" />
                                                        <TextBlock Margin="286,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="160"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=""
                                                                   x:Name="LabelStatusFreq"
                                                                   TextAlignment="Right" />
                                                        <TextBlock Margin="451,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="111"
                                                                   Height="22"
                                                                   DataContext="{Binding}"
                                                                   VerticalAlignment="Top"
                                                                   Text=""
                                                                   x:Name="LabelTimeFreq"
                                                                   TextAlignment="Right" />
                                                        <TextBlock Margin="267,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=":"
                                                                   x:Name="LabelStatusCharacter_Copy2"
                                                                   HorizontalAlignment="Left"
                                                                   Width="28" />
                                                        <TextBlock Margin="587,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="92"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="hh:mm:ss" />
                                                    </Grid>
                                                    <Grid Width="700"
                                                          Margin="0,5">
                                                        <TextBlock TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="253"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="Password Length"
                                                                   FontWeight="Bold" />
                                                        <TextBlock Margin="286,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="160"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=""
                                                                   x:Name="LabelStatusLength"
                                                                   TextAlignment="Right" />
                                                        <TextBlock Margin="451,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="111"
                                                                   Height="22"
                                                                   DataContext="{Binding}"
                                                                   VerticalAlignment="Top"
                                                                   Text=""
                                                                   x:Name="LabelTimeLength"
                                                                   TextAlignment="Right" />
                                                        <TextBlock Margin="267,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=":"
                                                                   x:Name="LabelStatusCharacter_Copy1"
                                                                   HorizontalAlignment="Left"
                                                                   Width="28" />
                                                        <TextBlock Margin="587,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="92"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="hh:mm:ss" />
                                                    </Grid>
                                                    <Grid Width="700"
                                                          Margin="0,5">

                                                        <TextBlock TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="253"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="Character Analysis"
                                                                   FontWeight="Bold" />
                                                        <TextBlock Margin="286,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=""
                                                                   x:Name="LabelStatusCharacter"
                                                                   HorizontalAlignment="Left"
                                                                   Width="160"
                                                                   TextAlignment="Right" />
                                                        <TextBlock Margin="451,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="111"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=""
                                                                   x:Name="LabelTimeCharacter"
                                                                   TextAlignment="Right" />
                                                        <TextBlock Margin="267,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text=":"
                                                                   x:Name="LabelStatusCharacter_Copy"
                                                                   HorizontalAlignment="Left"
                                                                   Width="28" />
                                                        <TextBlock Margin="587,0,0,0"
                                                                   TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="92"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="hh:mm:ss" />
                                                    </Grid>
                                                    <Grid Width="700"
                                                          Margin="0,5">
                                                        <TextBlock TextWrapping="WrapWithOverflow"
                                                                   FontSize="18"
                                                                   HorizontalAlignment="Left"
                                                                   Width="253"
                                                                   Height="22"
                                                                   VerticalAlignment="Top"
                                                                   Text="Common Base Words"
                                                                   FontWeight="Bold" />
