                                                    FontSize="18"
                                                    Foreground="White"
                                                    VerticalAlignment="Top"
                                                    Name="WordlistDisableAll"
                                                    Click="WordlistDisableAll_Click"
                                                    MinWidth="200"
                                                    MaxWidth="300"
                                                    HorizontalAlignment="Left" />
                                        </WrapPanel>
                                        <Grid Grid.Row="1"
                                              Margin="50,20,0,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="auto" />
                                                <RowDefinition Height="auto" />
                                                <RowDefinition Height="auto" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition MinWidth="150"
                                                                  Width="auto" />
                                                <ColumnDefinition Width="*" />

                                            </Grid.ColumnDefinitions>
                                            <CheckBox  Grid.Column="0"
                                                       Grid.Row="0"
                                                       Content="Prefix String:"
                                                       VerticalAlignment="Top"
                                                       Height="41"
                                                       IsChecked="False"
                                                       FontSize="22"
                                                       x:Name="CheckBoxWordlistPrefix"
                                                       HorizontalAlignment="Left" />
                                            <CheckBox Grid.Column="0"
                                                      Grid.Row="1"
                                                      Content="Suffix String:"
                                                      VerticalAlignment="Top"
                                                      Height="41"
                                                      IsChecked="False"
                                                      FontSize="22"
                                                      x:Name="CheckBoxWordlistSuffix"
                                                      HorizontalAlignment="Left" />
                                            <TextBox Grid.Column="1"
                                                     Grid.Row="0"
                                                     HorizontalAlignment="Stretch"
                                                     Height="32"
                                                     TextWrapping="Wrap"
                                                     Text=""
                                                     VerticalAlignment="Top"
                                                     x:Name="TextBoxWordlistPrefix"
                                                     FontSize="18" />
                                            <TextBox Grid.Column="1"
                                                     Grid.Row="1"
                                                     HorizontalAlignment="Stretch"
                                                     Height="32"
                                                     TextWrapping="Wrap"
                                                     Text=""
                                                     VerticalAlignment="Top"
                                                     x:Name="TextBoxWordlistSuffix"
                                                     FontSize="18" />

                                            <CheckBox Grid.Row="2"
                                                      Grid.ColumnSpan="2"
                                                      Content="Recursively Process Wordlist Generation *"
                                                      VerticalAlignment="Top"
                                                      Height="41"
                                                      IsChecked="false"
                                                      FontSize="22"
                                                      x:Name="CheckBoxWordlistRecursive"
                                                      HorizontalAlignment="Stretch" />
                                        </Grid>
                                    </Grid>
                                    <Grid Grid.Row="3"
                                          Margin="50,20,0,0">
                                        <TextBlock TextWrapping="WrapWithOverflow"
                                                   FontSize="18"
                                                   Text="* Enable this option to repeat each generation option against each enabled wordlist builder feature..Please note that this can greatly increase processing time, but also provides considerably better results for using in a wordlist password attack." />
                                    </Grid>

                                    <Grid Grid.Row="4"
                                          Margin="50,20,0,0"
                                          HorizontalAlignment="Center"
                                          VerticalAlignment="Bottom">
                                        <WrapPanel Orientation="Horizontal"
                                                   HorizontalAlignment="Stretch">

                                            <Button Margin="0,20,20,0"
                                                    MinWidth="210"
                                                    Content="Create From File"
                                                    Height="58"
                                                    FontSize="18"
                                                    Foreground="White"
                                                    Name="CreateWordlist"
                                                    Click="CreateWordlist_Click" />
                                            <Button Margin="0,20,20,0"
                                                    MinWidth="210"
                                                    Content="Create From Analysis"
                                                    Height="58"
                                                    FontSize="18"
                                                    Foreground="White"
                                                    x:Name="CreateWordlistAnalysis"
                                                    IsEnabled="False"
                                                    Click="CreateWordlistAnalysis_Click" />
                                            <Button Margin="0,20,20,0"
                                                    MinWidth="210"
                                                    Content="Save To File"
                                                    Height="59"
                                                    FontSize="18"
                                                    Foreground="White"
                                                    x:Name="SaveWordlist"
                                                    Click="SaveWordlist_Click"
                                                    IsEnabled="False" />
                                        </WrapPanel>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </ScrollViewer>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Width="{Binding ElementName=c1, Path=Width}"
                     Margin="-2,-2,2,2"
                     Header="About"
                     Name="TabAbout">
                <Grid Margin="48"
                      Width="auto"
                      Height="auto">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Row="0"
                               Grid.Column="0"
                               Grid.ColumnSpan="2"
                               Margin="0 20"
                               TextWrapping="Wrap">
            Pwdlyser is the password analysis and reporting tool for Active Directory, database exports, and a wide variety of other environments to automate the analysis and report of user account passwords, with an aim of providing consistent results for organisations, security consultants, and penetration testers.
                    </TextBlock>
                    <Label Content="Homepage"
                           Grid.Column="0"
                           Grid.Row="1"
                           Margin="0 10" />

                    <Label Content="GitHub"
                           Grid.Column="0"
                           Grid.Row="2"
                           Margin="0 10" />

                    <Label Content="License"
                           Grid.Column="0"
                           Grid.Row="3"
                           Margin="0 10" />

                    <Label Grid.Row="1"
                           Grid.Column="1"
                           Margin="0 10">
                        <Hyperlink NavigateUri="https://www.pwdlyser.com"
                                   RequestNavigate="Hyperlink_RequestNavigate">
                            https://www.pwdlyser.com
                        </Hyperlink>
                    </Label>
                    <Label Grid.Row="2"
                           Grid.Column="1"
                           Margin="0 10">
                        <Hyperlink NavigateUri="https://github.com/ins1gn1a/Pwdlyser"
                                   RequestNavigate="Hyperlink_RequestNavigate">
                            https://github.com/ins1gn1a/Pwdlyser
                        </Hyperlink>

                    </Label>
                    <Label Grid.Row="3"
                           Grid.Column="1"
                           Margin="0 10">
                        <Hyperlink NavigateUri="https://www.pwdlyser.com/license/"
                                   RequestNavigate="Hyperlink_RequestNavigate">
                            GNU General Public License v3.0
                        </Hyperlink>
                    </Label>
                </Grid>

            </TabItem>

        </TabControl>

    </Grid>
</Window>
